      <!-- Heading -->
      <div class="heading">
          <div class="text"><b>adio</b></div>
      </div>

      <!-- Picture Upload Form -->
      <div class="box">
        <form action="/upload" method="post" class="needs-validation" enctype="multipart/form-data"  novalidate></br>
            <h2>Add a new advertisement</h2>
            <h3></h3>
            <div class="form-group" style="padding-left: 10%">
                <label for="adAudio" style="font-size: 14px">Choose audio file:</label>
                <input type="file" class="form-control-file" name="photos" id="adAudio">
            </div>
            <input type="submit" value="Finish" class="btn btn-success">
        </form>
      </div>

